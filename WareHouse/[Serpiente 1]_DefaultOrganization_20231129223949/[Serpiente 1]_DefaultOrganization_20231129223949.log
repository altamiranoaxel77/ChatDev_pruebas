        elif self.direction == "down":
            y -= self.size
        elif self.direction == "left":
            x += self.size
        elif self.direction == "right":
            x -= self.size
        self.body.append((x, y))
    def reset(self):
        # Reset the snake to its initial state
        self.direction = "right"
        self.body = [(self.window_width // 2, self.window_height // 2)]
